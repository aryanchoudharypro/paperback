@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

if(NOT TARGET lexbor::lexbor)
	add_library(lexbor::lexbor STATIC IMPORTED)

	find_library(LEXBOR_LIBRARY_RELEASE
		NAMES lexbor_static
		PATHS "${CMAKE_CURRENT_LIST_DIR}/../../lib"
		NO_DEFAULT_PATH
	)
	find_library(LEXBOR_LIBRARY_DEBUG
		NAMES lexbor_static
		PATHS "${CMAKE_CURRENT_LIST_DIR}/../../debug/lib"
		NO_DEFAULT_PATH
	)

	set_target_properties(lexbor::lexbor PROPERTIES
		IMPORTED_LOCATION_RELEASE "${LEXBOR_LIBRARY_RELEASE}"
		IMPORTED_LOCATION_DEBUG "${LEXBOR_LIBRARY_DEBUG}"
		INTERFACE_INCLUDE_DIRECTORIES "${CMAKE_CURRENT_LIST_DIR}/../../include"
		IMPORTED_CONFIGURATIONS "RELEASE;DEBUG"
	)
	target_compile_definitions(lexbor::lexbor INTERFACE LEXBOR_STATIC)
endif()
